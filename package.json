{
  "name": "vulpes-celare",
  "version": "1.0.0",
  "description": "Vulpes Celare - A high-performance HIPAA PHI redaction engine. 99.4% sensitivity, 100% specificity.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && node scripts/copy-assets.js",
    "build:watch": "tsc --watch",
    "test": "node tests/master-suite/run.js",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:bench": "vitest bench",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [
    "hipaa",
    "phi",
    "redaction",
    "healthcare",
    "privacy",
    "pii",
    "medical",
    "compliance",
    "data-protection",
    "anonymization",
    "de-identification",
    "nlp",
    "text-processing"
  ],
  "author": "DocHatty",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/DocHatty/Vulpes-Celare.git"
  },
  "homepage": "https://github.com/DocHatty/Vulpes-Celare#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/string-similarity": "^4.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "typescript": "^5.0.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@flatten-js/interval-tree": "^2.0.3",
    "bloom-filters": "^3.0.4",
    "lru-cache": "^11.2.4"
  }
}
