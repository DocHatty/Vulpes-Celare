{
  "name": "vulpes-celare",
  "version": "1.0.0",
  "description": "Vulpes Celare - A high-performance HIPAA PHI redaction engine. 99.4% sensitivity, 100% specificity.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vulpes": "./dist/cli/launcher.js",
    "vulpes-cli": "./dist/cli/index.js",
    "vulpes-celare": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && node scripts/copy-assets.js",
    "build:watch": "tsc --watch",
    "test": "node tests/master-suite/run.js",
    "test:aggregate": "node tests/master-suite/run-aggregate.js",
    "test:aggregate:200": "node tests/master-suite/run-aggregate.js --batches 4 --size 50",
    "test:aggregate:500": "node tests/master-suite/run-aggregate.js --batches 10 --size 50",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:bench": "vitest bench",
    "test:coverage": "vitest run --coverage",
    "test:parquet": "node tests/master-suite/cortex/run-parquet-analysis.js",
    "test:parquet:quick": "node tests/master-suite/cortex/run-parquet-analysis.js --limit 100",
    "workflow:improve": "node tests/master-suite/cortex/improvement-workflow.js",
    "workflow:improve:dry": "node tests/master-suite/cortex/improvement-workflow.js --dry-run",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "install-global": "node scripts/install-global.js",
    "vulpes": "node dist/cli/launcher.js"
  },
  "keywords": [
    "hipaa",
    "phi",
    "redaction",
    "healthcare",
    "privacy",
    "pii",
    "medical",
    "compliance",
    "data-protection",
    "anonymization",
    "de-identification",
    "nlp",
    "text-processing"
  ],
  "author": "DocHatty",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/DocHatty/Vulpes-Celare.git"
  },
  "homepage": "https://github.com/DocHatty/Vulpes-Celare#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.25",
    "@types/string-similarity": "^4.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "typescript": "^5.0.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.7.0",
    "@flatten-js/interval-tree": "^2.0.3",
    "acp-sdk": "^1.0.3",
    "better-sqlite3": "^12.5.0",
    "bloom-filters": "^3.0.4",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "dcmjs": "^0.45.0",
    "dicom-parser": "^1.8.21",
    "double-metaphone": "^2.0.1",
    "execa": "^9.6.1",
    "fastest-levenshtein": "^1.0.16",
    "figures": "^6.1.0",
    "lru-cache": "^11.2.4",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "ora": "^9.0.0",
    "p-queue": "^9.0.1",
    "sharp": "^0.34.5",
    "zod": "^4.1.13"
  }
}
